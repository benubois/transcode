<ul class="discs">
{{#discs}}
	<li id="{{id}}">
		<div class="disc">
			{{name}}
			<div class="delete">
				<form action="/disc/{{id}}" method="post">
					<input name="_method" type="hidden" value="delete" />
					<button>Delete</button>
				</form>
			</div>
			<div class="titles">
				<div class="row clearfix">
					{{#titles}}
						<a href="/enqueue/{{id}}" {{#queued}}class="selected"{{/queued}} data-id="{{id}}">
							{{timecode}}
							<span class="progress-wrap">
								<span class="progress">
									<span class="progress-inner" style="width: {{#transcoded}}100{{/transcoded}}{{^transcoded}}{{progress}}{{/transcoded}}%;"></span>
								</span>
							</span>
						</a>
					{{/titles}}
				</div>
			</div>
		</div>
	</li>
{{/discs}}
</ul>