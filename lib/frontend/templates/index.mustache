<ul class="queue list">
	{{#queue}}
	<li data-id="{{id}}">
		
		<div class="wrap">
			<div class="content">			
				<div class="name">{{disc_name}}</div>
				<div class="meta">
					Title {{title}} 
					<span class="meta-light">{{timecode}}</span>
				</div>
				{{>progress}}
			</div>
			<div class="sidebar">
				<a href="/unqueue/{{id}}" class="button-delete">Delete</a>
			</div>			
		</div>

	</li>
	{{/queue}}
	{{^queue}}
	<li>
		Nothing in the queue
	</li>
	{{/queue}}
</ul>